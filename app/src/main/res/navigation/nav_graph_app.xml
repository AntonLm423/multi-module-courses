<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_app"
    app:startDestination="@id/nav_graph_onboarding">

    <include app:graph="@navigation/nav_graph_onboarding" />

    <include app:graph="@navigation/nav_graph_auth" />

    <include app:graph="@navigation/nav_graph_main" />

    <include app:graph="@navigation/nav_graph_favorite" />

    <action
        android:id="@+id/action_global_nav_graph_main"
        app:destination="@id/nav_graph_main" />

    <action
        android:id="@+id/action_global_nav_graph_favorite"
        app:destination="@id/nav_graph_favorite" />
</navigation>